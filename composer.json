{
    "name": "niccolomeloni/lara-football-data",
    "description": "Use football-data.org API in your Laravel application",
    "keywords": ["laravel", "football", "data", "api"],
    "type": "library",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niccol√≤ Meloni",
            "email": "niccolomeloni@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Niccolomeloni\\LaraFootballData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niccolomeloni\\LaraFootballData\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niccolomeloni\\LaraFootballData\\ApiFootballDataServiceProvider"
            ],
            "aliases": {
                "LaraFootballData": "Niccolomeloni\\LaraFootballData\\Facades\\ApiFootballData"
            }
        }
    }
}
